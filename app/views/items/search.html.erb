<div class="col-md-offset-1 col-md-4">
  <h1>Search</h1>
    <%= form_tag(do_search_items_path, method: "get", class="form") do %>
      <div class="form-group">
        <label>Title</label>
        <%= text_field_tag(:title, "", class="form-control") %>
      </div>

      <%%= submit_tag("Search", class="btn btn-default") %>
    <% end %>
</div>
